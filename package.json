  "main": "index.ts",
  "type": "module",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jest ./test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lika85456/crud-storage.git"
